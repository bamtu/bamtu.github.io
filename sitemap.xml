---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in site.posts %}
  <url>
    <loc>{{ site.url }}{{ post.url }}</loc>
    <lastmod>{{ post.last_modified_at | default: post.date | date_to_xmlschema }}</lastmod>
    <changefreq>{{ post.sitemap.changefreq | default: site.sitemap.changefreq }}</changefreq>
    <priority>{{ post.sitemap.priority | default: site.sitemap.priority }}</priority>
  </url>
  {% endfor %}
</urlset>